class ImageLibrary {
	constructor() {
		this.images = {};
	}
	
	function get_image(path) {
		img = this.images.get(path, null);
		if (img == null) {
			$gfx_image_load_from_resource('t', 'images/' + path);
			img = $gfx_image_get('t');
			this.images[path] = img;
		}
		return img;
	}
}

IMAGE_LIBRARY = new ImageLibrary();