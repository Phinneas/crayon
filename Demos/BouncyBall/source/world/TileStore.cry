TILE_IDS = 'x g'.split(' ');

class TileStore {
	constructor() {
		this.templates = null;
	}
	
	function initialize() {
		if (this.templates == null) {
			this.templates = {};
			for (id : TILE_IDS) {
				template = new TileTemplate(id, [id + '.png']);
				this.templates[id] = template;
			}
		}
	}
	
	function get(id) {
		return this.templates[id];
	}
}
TILE_STORE = new TileStore();
