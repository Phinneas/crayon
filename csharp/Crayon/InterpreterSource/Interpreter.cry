						list1 = $_cast('List<Value>', Value$root.internalValue);
						len = $_list_length(list1);
					} else if (type == Types.DICTIONARY) {
						dictImpl = $_cast('DictImpl', Value$root.internalValue);
						len = DictImpl$dictImpl.size;
					} else if (type == Types.STRING) {
