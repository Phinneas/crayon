	Core.RegisterLibraryFunction(functionPointers, functionNames, argCounts, "directoryDelete", 2);
	Core.RegisterLibraryFunction(functionPointers, functionNames, argCounts, "directoryList", 4);
	Core.RegisterLibraryFunction(functionPointers, functionNames, argCounts, "directoryMove", 3);
	Core.RegisterLibraryFunction(functionPointers, functionNames, argCounts, "fileDelete", 2);
	Core.RegisterLibraryFunction(functionPointers, functionNames, argCounts, "fileInfo", 4);
	Core.RegisterLibraryFunction(functionPointers, functionNames, argCounts, "fileMove", 5);
	Core.RegisterLibraryFunction(functionPointers, functionNames, argCounts, "fileRead", 4);
	Core.RegisterLibraryFunction(functionPointers, functionNames, argCounts, "fileWrite", 4);
	Core.RegisterLibraryFunction(functionPointers, functionNames, argCounts, "getCurrentDirectory", 0);
	Core.RegisterLibraryFunction(functionPointers, functionNames, argCounts, "getUserDirectory", 0);
	Core.RegisterLibraryFunction(functionPointers, functionNames, argCounts, "initializeDisk", 2);
	Core.RegisterLibraryFunction(functionPointers, functionNames, argCounts, "isWindows", 0);
	Core.RegisterLibraryFunction(functionPointers, functionNames, argCounts, "textToLines", 2);
}
