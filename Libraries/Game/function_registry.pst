void lib_manifest_RegisterFunctions(List<object> functionPointers, List<string> functionNames, List<int> argCounts) {
	Core.RegisterLibraryFunction(functionPointers, functionNames, argCounts, "clock_tick", 0);
	Core.RegisterLibraryFunction(functionPointers, functionNames, argCounts, "initialize", 1);
	Core.RegisterLibraryFunction(functionPointers, functionNames, argCounts, "initialize_screen", 4);
	Core.RegisterLibraryFunction(functionPointers, functionNames, argCounts, "pump_events", 1);
	Core.RegisterLibraryFunction(functionPointers, functionNames, argCounts, "set_title", 1);
}
